{
  "manifest_version": 2,
  "name": "Web Scraper",
  "version": "1.0",
  "description": "Scrape and display content from a website",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
